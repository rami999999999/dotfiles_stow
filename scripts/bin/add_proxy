#!/bin/sh

makdir -p /etc/profile.d

cat <<EOF >>/etc/zsh/zshrc >>/etc/profile.d/nokia-proxy.sh
export HTTPS_PROXY=http://10.144.1.10:8080
export HTTP_PROXY=http://10.144.1.10:8080
export FTP_PROXY=http://10.144.1.10:8080
export http_proxy=http://10.144.1.10:8080
export https_proxy=http://10.144.1.10:8080
export ftp_proxy=http://10.144.1.10:8080
EOF
